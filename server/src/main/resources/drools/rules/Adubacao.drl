rule "Diagnostico_teores_argila_20"
    when
        Laudo(argila <= 20)
    then
        diagnostico.setClasseArgila(4);
end

rule "Diagnostico_teores_argila_21_40"
    when
        Laudo(argila >= 21 && argila <= 40)
    then
        diagnostico.setClasseArgila(3);
end

rule "Diagnostico_teores_argila_41_60"
    when
        Laudo(argila >= 41 && argila <= 60)
    then
        diagnostico.setClasseArgila(2);
end

rule "Diagnostico_teores_argila_60"
    when
        Laudo(argila > 60)
    then
        diagnostico.setClasseArgila(1);
end

rule "Diagnostico_teores_MO_25"
    when
        Laudo(materiaOrganicaMO <= 2.5)
    then
        diagnostico.setClasseMO("Baixo");
end

rule "Diagnostico_teores_MO_26_50"
    when
        Laudo(materiaOrganicaMO >= 2.6 && materiaOrganicaMO <= 5.0)
    then
        diagnostico.setClasseMO("Médio");
end

rule "Diagnostico_teores_MO_50"
    when
        Laudo(materiaOrganicaMO > 5.0)
    then
        diagnostico.setClasseMO("Alto");
end

rule "Diagnostico_teores_CTC_75"
    when
        Laudo(ctc <= 7.5)
    then
        diagnostico.setClasseCTC("Baixo");
end

rule "Diagnostico_teores_CTC_76_15"
